{
  "name": "offline-event-sourcing-application",
  "version": "0.0.0",
  "description": "An example app for offline syncing with event sourcing",
  "main": "dist/server.js",
  "repository": "git@github.com:TobiasWalle/offline-event-sourcing-application.git",
  "author": "Tobias Walle",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly src/server/main.ts",
    "build": "run-s clean build-client build-server build-functions",
    "build-functions": "netlify-lambda build dist/server/aws-functions",
    "build-server": "babel src/server -d dist/server --extensions '.ts'",
    "build-client": "shx mkdir -p ./dist/client && shx cp -r ./src/client ./dist",
    "clean": "shx rm -rf ./dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-typescript": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "body-parser": "^1.19.0",
    "netlify-lambda": "^1.4.10",
    "npm-run-all": "^4.1.5",
    "serverless-http": "^2.0.2",
    "shx": "^0.3.2",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0"
  },
  "dependencies": {
    "express": "^4.17.0"
  }
}
